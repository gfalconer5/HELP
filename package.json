    "@tensorflow/tfjs-backend-wasm": "^1.7.4",
    "@tensorflow/tfjs-converter": "^1.7.4",
    "@tensorflow/tfjs-core": "^1.7.4"
  },
  "devDependencies": {
    "rollup": "^2.18.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "servez": "^1.10.0"
  },
  "keywords": [
    "3d",
    "parallax",
    "deepview",
    "display",
    "face",
    "view",
    "motion"
