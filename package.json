{
  "name": "parallax-view",
  "version": "1.0.0",
  "description": "Parallax effect for web using face view. Image-based 3d display with tfjs.",
  "main": "src/parallax-view.js",
  "browser": "dist/parallax-view.js",
  "module": "dist/parallax-view.mjs",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "start": "servez -S"
  },
  "dependencies": {
